<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upn-org:service-1-0">
	<specVersion>
		<major>1</major>
		<minor>0</minor>
	</specVersion>
	<serviceStateTable>
		<stateVariable sendEvents="yes">
			<name>Enabled</name>
			<dataType>boolean</dataType>
			<defaultValue>0</defaultValue>
			<shortCode>Enabled</shortCode>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>LightsOutTime</name>
			<dataType>time</dataType>
			<defaultValue>nil</defaultValue>
			<shortCode>lightsout</shortCode>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>controlCount</name>
			<dataType>ui2</dataType>
			<defaultValue>0</defaultValue>
			<shortCode>ctrlcount</shortCode>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>MinCycleDelay</name>
			<dataType>ui4</dataType>
			<defaultValue>300</defaultValue>
			<shortCode>mincycle</shortCode>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>MaxCycleDelay</name>
			<dataType>ui4</dataType>
			<defaultValue>1800</defaultValue>
			<shortCode>maxcycle</shortCode>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>MinOffDelay</name>
			<dataType>ui4</dataType>
			<defaultValue>300</defaultValue>
			<shortCode>minoff</shortCode>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>MaxOffDelay</name>
			<dataType>ui4</dataType>
			<defaultValue>maxoff</defaultValue>
			<shortCode>maxoff</shortCode>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>TestMode</name>
			<dataType>boolean</dataType>
			<defaultValue>0</defaultValue>
			<shortCode>testmode</shortCode>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>TestSunset</name>
			<dataType>ui4</dataType>
			<defaultValue>nil</defaultValue>
			<shortCode>testsunset</shortCode>
		</stateVariable>
	</serviceStateTable>
	<actionList>
		<action>
			<name>SetEnabled</name>
			<argumentList>
				<name>NewEnabledValue</name>
				<relatedStateVariable>Enabled</relatedStateVariable>
				<direction>in</direction>
			</argumentList>
		</action>
	</actionList>
</scpd>